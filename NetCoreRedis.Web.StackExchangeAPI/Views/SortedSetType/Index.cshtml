@model SortedSet<string>
@{
    ViewData["Title"] = "Index";
}

<form asp-action="Add" asp-controller="SortedSetType" method="post" enctype="application/x-www-form-urlencoded">
    <div class="form-group">
        <label for="name" class="col-form-label">Name</label>
        <input type="text" name="name" class="form-control" />
    </div>
    <div class="form-group">
        <label for="name" class="col-form-label">Score</label>
        <input type="number" name="score" class="form-control" />
    </div>
    <div class="form-group">
        <input type="submit" value="Kaydet" class="btn btn-primary" />
    </div>
</form>

@foreach (var item in Model)
{
    <h2>@item <a asp-controller="SortedSetType" asp-action="Remove" asp-route-name="@item" class="btn btn-danger">Sil</a></h2>
}